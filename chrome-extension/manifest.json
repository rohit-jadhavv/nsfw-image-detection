{
    "manifest_version": 3,
    "name": "Image Extractor 2.0",
    "version": "1.0",
    "description": "Extracts images from the web page",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  